{
  "manifest_version": 3,
  "name": "Choomame",
  "version": "1.0.0",
  "permissions": ["unlimitedStorage", "storage"],
  "background": { "service_worker": "src/background/index.ts" },
  "content_scripts": [
    {
      "js": ["src/content/index.tsx"],
      "matches": ["https://www.google.com/*"]
    }
  ]
}
